<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.PfadiTechnik"
        tools:targetApi="31">
        <activity
            android:name="com.pfadi.pfaditechnik.Sonstiges"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name="com.pfadi.pfaditechnik.Karte"
            android:screenOrientation="portrait"
            android:exported="false"/>
        <activity
            android:name="com.pfadi.pfaditechnik.Natur"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name="com.pfadi.pfaditechnik.Ubermittlung"
            android:exported="false" />
        <activity
            android:name="com.pfadi.pfaditechnik.Pionier"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name="com.pfadi.pfaditechnik.Samariter"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name="com.pfadi.pfaditechnik.Pfadigeschichte"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name="com.pfadi.pfaditechnik.MainActivity"
            android:screenOrientation="portrait"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>